{
 "name": "@maana-io/maana-ai-goap",
 "private": true,
 "author": "Maana, Inc.",
 "license": "MIT",
 "version": "0.0.2",
 "description": "Maana Goal Oriented Action Planning",
 "main": "src/start.js",
 "repository": "git@github.com:maana-io/maana-ai-goap",
 "scripts": {
  "docker-build": "if [ -z \"$(git status --porcelain)\" ]; then docker build -t maana-ai-goap-logic:\"$(git rev-parse --short HEAD)\" .; else ECHO \"docker repository is not pristine\"; fi",
  "docker-run": "docker run -p 8050:8050 -p 8051:8051 --env-file .env -ti maana-ai-goap-logic:\"$(git rev-parse --short HEAD)\"",
  "docker-push": "if [ -z \"$(git status --porcelain)\" ]; then docker tag maana-ai-goap-logic:\"$(git rev-parse --short HEAD)\" docker.pkg.github.com/maana-io/goap-deployment/maana-ai-goap-logic:\"$(git rev-parse --short HEAD)\"; docker push docker.pkg.github.com/maana-io/goap-deployment/maana-ai-goap-logic:\"$(git rev-parse --short HEAD)\"; else ECHO \"git repository is not pristine\"; fi",
  "docker-release": "if [ -z \"$(git status --porcelain)\" ]; then docker tag maana-ai-goap-logic:\"$(git rev-parse --short HEAD)\" docker.pkg.github.com/maana-io/goap-deployment/maana-ai-goap-logic:$npm_package_version; docker push docker.pkg.github.com/maana-io/goap-deployment/maana-ai-goap-logic:$npm_package_version; else ECHO \"git repository is not pristine\"; fi",
  "precommit": "lint-staged",
  "prettier": "./node_modules/.bin/prettier --write \"src/**/*.{js,jsx}\"",
  "start": "babel-node ./src/start.dev.js",
  "dbg": "nodemon --inspect ./src/start.dev.js --exec babel-node -e js",
  "dev": "nodemon ./src/start.dev.js --exec babel-node -e js",
  "test": "NODE_ENV=test ava",
  "coverage": "NODE_ENV=test c8 --reporter html --all npm run test"
 },
 "lint-staged": {
  "src/**/*.{js,jsx,json,css}": [
   "prettier --write",
   "git add"
  ]
 },
 "dependencies": {
  "@babel/cli": "^7.7.7",
  "@babel/core": "^7.7.7",
  "@babel/node": "^7.7.7",
  "@babel/plugin-proposal-class-properties": "^7.7.4",
  "@babel/plugin-proposal-decorators": "^7.7.4",
  "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
  "@babel/plugin-proposal-function-sent": "^7.7.4",
  "@babel/plugin-proposal-numeric-separator": "^7.7.4",
  "@babel/plugin-proposal-throw-expressions": "^7.7.4",
  "@babel/plugin-syntax-dynamic-import": "^7.7.4",
  "@babel/plugin-syntax-import-meta": "^7.7.4",
  "@babel/plugin-transform-async-to-generator": "^7.7.4",
  "@babel/plugin-transform-runtime": "^7.7.6",
  "@babel/preset-env": "^7.7.7",
  "@babel/runtime": "^7.7.7",
  "@dagrejs/graphlib": "^2.1.4",
  "apollo-server-express": "^2.9.15",
  "auth0": "^2.20.0",
  "body-parser": "^1.19.0",
  "chalk": "^3.0.0",
  "collections": "^5.1.11",
  "cors": "^2.8.5",
